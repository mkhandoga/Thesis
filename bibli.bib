@Article{Wxsec2017,
	author={Aaboud, M. et al.},
	title={Precision measurement and interpretation of inclusive {\$}{\$}W^+{\$}{\$}W+, {\$}{\$}W^-{\$}{\$}W-and {\$}{\$}Z/{\backslash}gamma ^*{\$}{\$}Z/$\gamma$∗production cross sections with the ATLAS detector},
	journal={The European Physical Journal C},
